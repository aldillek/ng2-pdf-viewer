{
  "name": "ng2-pdf-viewer",
  "version": "10.4.0",
  "description": "Angular 5+ component for rendering PDF",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "nx serve",
    "build": "nx build --configuration production",
    "build:stats": "nx build --stats-json",
    "watch": "nx build --watch --configuration development",
    "analyze": "webpack-bundle-analyzer dist/stats.json",
    "test": "nx test --watch false",
    "test:ci": "nx test --watch=false --browsers=ChromeHeadlessCustom",
    "test:w": "nx test",
    "lint": "nx lint",
    "e2e": "nx e2e",
    "packagr": "ng-packagr -p ng-package.json",
    "docs": "nx build --configuration production --output-path docs --base-href ng2-pdf-viewer && cp ./docs/index.html ./docs/404.html"
  },
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://vadimdez@github.com/VadimDez/ng2-pdf-viewer.git"
  },
  "keywords": [
    "angular",
    "pdf",
    "angular",
    "angular-pdf",
    "angular2-pdf",
    "ng2",
    "ng2-pdf",
    "angular5"
  ],
  "author": "Vadym Yatsyuk <vadym.yatsyuk@gmail.com>",
  "bugs": {
    "url": "https://github.com/VadimDez/ng2-pdf-viewer/issues"
  },
  "homepage": "https://github.com/VadimDez/ng2-pdf-viewer#readme",
  "dependencies": {
    "pdfjs-dist": "4.8.69",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@analogjs/vite-plugin-angular": "~1.10.0",
    "@analogjs/vitest-angular": "~1.10.0",
    "@angular-devkit/build-angular": "^16.1.0",
    "@angular-devkit/core": "~16.1.0",
    "@angular-devkit/schematics": "~16.1.0",
    "@angular/animations": "^16.1.0",
    "@angular/cli": "~16.1.0",
    "@angular/common": "^16.1.0",
    "@angular/compiler": "^16.1.0",
    "@angular/compiler-cli": "^16.1.0",
    "@angular/core": "^16.1.0",
    "@angular/forms": "^16.1.0",
    "@angular/language-service": "16.1.0",
    "@angular/material": "^16.0.4",
    "@angular/platform-browser": "^16.1.0",
    "@angular/platform-browser-dynamic": "^16.1.0",
    "@angular/router": "^16.1.0",
    "@eslint/js": "^9.8.0",
    "@nx/angular": "20.4.6",
    "@nx/eslint": "20.4.6",
    "@nx/eslint-plugin": "20.4.6",
    "@nx/js": "20.4.6",
    "@nx/vite": "20.4.6",
    "@nx/web": "20.4.6",
    "@nx/workspace": "20.4.6",
    "@schematics/angular": "~16.1.0",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@types/jasmine": "~4.3.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^20.3.1",
    "@typescript-eslint/utils": "^8.19.0",
    "@vitest/coverage-v8": "^1.0.4",
    "@vitest/ui": "^1.3.1",
    "angular-eslint": "^19.0.2",
    "codelyzer": "^6.0.2",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^9.0.0",
    "jasmine-core": "^4.6.0",
    "jasmine-spec-reporter": "^7.0.0",
    "jsdom": "~22.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^16.1.0",
    "nx": "20.4.6",
    "prettier": "^2.6.2",
    "protractor": "~7.0.0",
    "rxjs": "~7.8.0",
    "tslint": "^6.1.3",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.19.0",
    "vite": "^5.0.0",
    "vitest": "^1.3.1",
    "webpack-bundle-analyzer": "^4.7.0",
    "zone.js": "~0.13.0"
  }
}
