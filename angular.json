{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ng2-pdf-viewer-docs": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/ng2-pdf-viewer-docs",
      "sourceRoot": "projects/ng2-pdf-viewer-docs/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/ng2-pdf-viewer-docs",
            "index": "projects/ng2-pdf-viewer-docs/src/index.html",
            "main": "projects/ng2-pdf-viewer-docs/src/main.ts",
            "polyfills": ["zone.js"],
            "tsConfig": "projects/ng2-pdf-viewer-docs/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/ng2-pdf-viewer-docs/src/favicon.ico",
              "projects/ng2-pdf-viewer-docs/src/assets"
            ],
            "styles": ["projects/ng2-pdf-viewer-docs/src/styles.scss"],
            "scripts": [],
            "allowedCommonJsDependencies": [
              "pdfjs-dist",
              "pdfjs-dist/web/pdf_viewer"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "ng2-pdf-viewer-docs:build:production"
            },
            "development": {
              "buildTarget": "ng2-pdf-viewer-docs:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "ng2-pdf-viewer-docs:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": ["zone.js", "zone.js/testing"],
            "tsConfig": "projects/ng2-pdf-viewer-docs/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/ng2-pdf-viewer-docs/src/favicon.ico",
              "projects/ng2-pdf-viewer-docs/src/assets"
            ],
            "styles": ["projects/ng2-pdf-viewer-docs/src/styles.scss"],
            "scripts": []
          }
        }
      }
    },
    "ng2-pdf-viewer": {
      "projectType": "library",
      "root": "projects/ng2-pdf-viewer",
      "sourceRoot": "projects/ng2-pdf-viewer/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/ng2-pdf-viewer/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ng2-pdf-viewer/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/ng2-pdf-viewer/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/ng2-pdf-viewer/tsconfig.spec.json",
            "polyfills": ["zone.js", "zone.js/testing"]
          }
        }
      }
    },
    "pdf-viewer": {
      "projectType": "library",
      "root": "projects/pdf-viewer",
      "sourceRoot": "projects/pdf-viewer/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/pdf-viewer/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/pdf-viewer/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/pdf-viewer/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/pdf-viewer/tsconfig.spec.json",
            "polyfills": ["zone.js", "zone.js/testing"]
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}
